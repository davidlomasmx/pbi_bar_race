            "fill": {
              "solid": {
                "color": true
              }
            }
          }
        },
        "textColor": {
          "displayName": "Text color",
          "type": {
            "fill": {
              "solid": {
                "color": true
              }
            }
          }
        },
        "fontFamily": {
          "displayName": "Font Family",
          "type": {
            "formatting": {
              "fontFamily": true
            }
          }
        },
        "yearSize": {
          "displayName": "Year Label Size",
          "type": {
            "numeric": true
          }
        },
        "monthSize": {
          "displayName": "Month Label Size",
          "type": {
            "numeric": true
          }
        },
        "barsToShow": {
          "displayName": "Top N Rank",
          "type": {
            "numeric": true
          }
        },
        "intervalTiming": {
          "displayName": "Interval (seconds)",
          "type": {
            "numeric": true
          }
        },
        "valueFormat": {
          "displayName": "Value Format",
          "type": {
            "text": true
          }
        }
      }
    }
  },
  "dataViewMappings": [
    {
      "table": {
        "rows": {
          "select": [
            {
              "for": {
